<% if (true == Rails.env.development?) %>
  <% minified = "" %>
<% else %>
  <% minified = ".min" %>
<% end %>

<% jasmine ||= false %>

<% versions = {
  :angular => "1.6.4",
  :angular_ui => "2.5.0",
  :underscore => "1.8.3",
  :bootstrap  => "3.3.7",
  :jquery => "3.2.1",
  :restangular => "1.5.1"
}
%>

<% if (false == jasmine) %>
  <%= render :partial => "common/header/stylesheet_includes", :locals => {:versions => versions, :minified => minified} %>
<% end %>

<%= render :partial => "common/header/javascript_includes", :locals => {:versions => versions, :minified => minified} %>
